{
  "name": "magnetic-v1",
  "version": "1.0.0",
  "description": "GSAP-powered magnetic cursor animations for React",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "demo": "vite --config vite.demo.config.ts",
    "lint": "eslint src/ demo/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ demo/ --ext .ts,.tsx --fix",
    "lint:check": "eslint src/ demo/ --ext .ts,.tsx --max-warnings 0",
    "prepublishOnly": "npm run build",
    "publish:beta": "npm publish --tag beta"
  },
  "keywords": [
    "react",
    "gsap",
    "cursor",
    "magnetic",
    "animation",
    "component"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/magnetic-v1"
  },
  "peerDependencies": {
    "gsap": ">=3.12.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "gsap": ">=3.12.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.35.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sass": "^1.92.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-lib-inject-css": "^2.1.1"
  }
}
